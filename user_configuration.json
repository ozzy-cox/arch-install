{
    "additional-repositories": [
        "multilib"
    ],
    "audio": "pipewire",
    "bootloader": "systemd-bootctl",
    "config_version": "2.5.1",
    "debug": false,
    "harddrives": [
        "[drive-name]"
    ],
    "hostname": "ozank",
    "nic": {
        "dhcp": true,
        "dns": null,
        "gateway": null,
        "iface": null,
        "ip": null,
        "type": "nm"
    },
    "no_pkg_lookups": false,
    "ntp": true,
    "offline": false,
    "packages": [
        "git",
        "wget",
        "vim",
        "base-devel",
        "openssl",
        "openssh",
        "rofi",
        "feh",
        "chromium",
        "alacritty",
        "fzf",
        "lshw",
        "picom",
        "xorg-server",
        "xorg-xrandr",
        "xorg-xinit",
        "xorg-xsetroot",
        "arandr",
        "htop",
        "zsh",
        "vicious",
        "reflector",
        "ttf-fira-sans",
        "ttf-roboto",
        "docker",
        "pavucontrol", "alsa-firmware", "sof-firmware", "alsa-ucm-conf",
        "zip", "unzip",
        "openresolv", "iwd", "wireguard-tools", "makepkg"
    ],
    "profile": {
        "path": "/usr/lib/python3.10/site-packages/archinstall/profiles/minimal.py"
    },
    "script": "guided",
    "silent": false,
    "swap": true,
    "version": "2.5.1",
    "custom-commands": [
        "git clone https://github.com/ozzy-cox/dotfiles.git /home/ozank/.dotfiles",
        "git clone https://aur.archlinux.org/paru.git /opt/paru"
    ]
}